{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "lib": [
            "DOM",
            "es7",
            "es2017.object",
        ],
        "jsx": "react",
        "allowJs": true,
        "sourceMap": true,
        "outDir": "./build",
        "rootDir": "../../",
        "strict": false,
        "strictNullChecks": false,
        "alwaysStrict": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["../../*"]
        },
        "types": ["jquery"],
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": [
        "./*"
    ],
    "exclude": []
}